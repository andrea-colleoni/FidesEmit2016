  <display-name>HelloService</display-name>
  <enterprise-beans>
    <session>
      <display-name>HelloServiceSB</display-name>
      <ejb-name>HelloService</ejb-name>
      <service-endpoint>hello.HelloServiceSEI</service-endpoint>
