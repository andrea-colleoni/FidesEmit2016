<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar version="2.1" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd">
  <display-name>Enroller</display-name>
  <enterprise-beans>
    <session>
      <display-name>EnrollerSB</display-name>
      <ejb-name>EnrollerBean</ejb-name>
      <home>enroller.EnrollerRemoteHome</home>
      <remote>enroller.EnrollerRemote</remote>
      <ejb-class>enroller.EnrollerBean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
    </session>
    <entity>
      <display-name>StudentEB</display-name>
      <ejb-name>StudentBean</ejb-name>
      <home>enroller.StudentRemoteHome</home>
      <remote>enroller.StudentRemote</remote>
      <ejb-class>enroller.StudentBean</ejb-class>
      <persistence-type>Bean</persistence-type>
      <prim-key-class>java.lang.String</prim-key-class>
      <reentrant>false</reentrant>
      <ejb-ref>
        <ejb-ref-name>ejb/SimpleEnroller</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <home>enroller.EnrollerRemoteHome</home>
        <remote>enroller.EnrollerRemote</remote>
        <ejb-link>EnrollerBean</ejb-link>
      </ejb-ref>
    </entity>
    <entity>
      <display-name>CourseEB</display-name>
      <ejb-name>CourseBean</ejb-name>
      <home>enroller.CourseRemoteHome</home>
      <remote>enroller.CourseRemote</remote>
      <ejb-class>enroller.CourseBean</ejb-class>
      <persistence-type>Bean</persistence-type>
      <prim-key-class>java.lang.String</prim-key-class>
      <reentrant>false</reentrant>
      <ejb-ref>
        <ejb-ref-name>ejb/SimpleEnroller</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <home>enroller.EnrollerRemoteHome</home>
        <remote>enroller.EnrollerRemote</remote>
        <ejb-link>EnrollerBean</ejb-link>
      </ejb-ref>
    </entity>
  </enterprise-beans>
  <assembly-descriptor>
  </assembly-descriptor>
</ejb-jar>
