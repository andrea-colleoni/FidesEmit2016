<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 8.1 EJB 2.1//EN" "http://www.sun.com/software/appserver/dtds/sun-ejb-jar_2_1-1.dtd">
<sun-ejb-jar>
  <enterprise-beans>
    <ejb>
      <ejb-name>RequestBean</ejb-name>
      <jndi-name>ejb/RequestBean</jndi-name>
    </ejb>
    <cmp-resource>
      <jndi-name>jdbc/pointbase</jndi-name>
      <schema-generator-properties>
        <property>
          <name>PartBean.specification.jdbc-type</name>
          <value>CLOB</value>
        </property>
      </schema-generator-properties>
    </cmp-resource>
  </enterprise-beans>
</sun-ejb-jar>
