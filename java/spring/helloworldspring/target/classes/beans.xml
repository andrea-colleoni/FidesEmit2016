<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="helloWorld" class="com.gianluca.helloworldspring.HelloWorld">
		<property name="message" value="Benvenuti a Sarabamba!" />
	</bean>
	
	<bean id="moseca" class="com.gianluca.helloworldspring.HelloWorld">
		<property name="message" value="Moooseca!" />
	</bean>

</beans>
